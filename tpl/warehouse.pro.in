QT += core gui

greaterThan(QT_MAJOR_VERSION, 4): QT += widgets multimedia

CONFIG         += c++14
QMAKE_CXXFLAGS += -g3 -Wall -Wextra
INCLUDEPATH    += src
DEFINES        += QTCREATOR QT_DEPRECATED_WARNINGS

DISTFILES += \ 
    carte.txt \ 
    niveaux.xsb \ 
    xsokoban.xsb \ 
    yoshio.xsb \ 
    microban1.xsb \ 
    microban2.xsb

RESOURCES += \
	res/tileset/res-iso.qrc \
	res/images/images.qrc

HEADERS += @HEADERS@
SOURCES += @SOURCES@
